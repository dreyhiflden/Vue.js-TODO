>\n    <button v-if=\"!isEditing\" @click=\"toggleEditingState\">Edit</button>\n    <button v-if=\"isEditing\" @click=\"edit(todo)\">Save</button>\n    <button @click=\"removeTodoItemMethod(todo)\">Delete</button>\n  </li>\n</template>\n\n<script>\n  import { mapActions } from \"vuex\";\n\n  export default {\n    name: \"TodoListItem\",\n    data() {\n      return {\n        isEditing: false,\n        bufferingInputText: ''\n      }\n    },\n    computed: {\n      checked() {\n        return this.todo.completed;\n      }\n    },\n    props: {\n      todo: Object\n    },\n    methods: {\n      ...mapActions(['toggleCompletedStatus', 'editTodoItem', 'removeTodoItem']),\n\n      toggleCompletedStatusMethod(todo) {\n        this.toggleCompletedStatus(todo)\n      },\n      toggleEditingState() {\n        this.isEditing = !this.isEditing;\n      },\n      // edit(todo){\n      //   console.log(todo);\n      //   this.$store.dispatch('editTodoItem', todo);\n      //   this.toggleEditingState();\n      // },\n      edit(todo){\n        // console.log(todo);\n        // this.$store.dispatch('editTodoItem', this.bufferingInputText);\n        this.editTodoItem({\n          newText : this.bufferingInputText,\n          todoId : this.todo.id\n        });\n        this.$store.dispatch('editTodoItem', this.bufferingInputText);\n        this.toggleEditingState();\n      },\n      removeTodoItemMethod(todo) {\n        this.removeTodoItem(todo);\n      },\n      bufferingText(e) {\n        this.bufferingInputText = e.target.value;\n        // console.log(e.target.value);\n        console.log(this.bufferingInputText);\n        // this.$store.dispatch('editTodoItem', e.target.value);\n        // this.text =\n      }\n    },\n  }\n</script>\n\n<style scoped lang=\"scss\">\n  .todo-list__item {\n    list-style: none;\n  }\n\n  .todo-list__item--completed {\n    text-decoration: line-through;\n  }\n\n  .item-name {\n    display: inline-block;\n    width: 300px;\n  }\n</style>\n"]}]}